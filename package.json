{
  "name": "studtwork_desk",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --mode development --port 9000",
    "build": "vue-tsc && vite build --mode production",
    "preview": "vite preview",
    "format": "prettier --write ./**/*.{vue,ts,tsx,js,jsx,css,less,scss,json,md}",
    "prepare": "husky install",
    "cz": "git cz"
  },
  "lint-staged": {
    "*.{js,vue,ts,jsx,tsx}": [
      "prettier --write"
    ],
    "*.{html,css,less,scss,md}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "dependencies": {
    "@vueuse/core": "^10.3.0",
    "@vueuse/head": "^1.1.26",
    "axios": "^1.4.0",
    "nprogress": "^0.2.0",
    "vue": "^3.3.4",
    "vue-lazyload": "^3.0.0",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@bytemd/plugin-gfm": "^1.21.0",
    "@bytemd/vue-next": "^1.21.0",
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@types/node": "^20.4.8",
    "@types/nprogress": "^0.2.0",
    "@unhead/vue": "^1.2.2",
    "@unocss/preset-icons": "^0.54.1",
    "@vitejs/plugin-vue": "^4.2.3",
    "cz-git": "^1.7.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "normalize.css": "^8.0.1",
    "prettier": "^3.0.1",
    "typescript": "^5.0.2",
    "unocss": "^0.54.1",
    "unplugin-auto-import": "^0.16.6",
    "vite": "^4.4.5",
    "vite-plugin-checker": "^0.6.1",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-vue-devtools": "^1.0.0-beta.6",
    "vite-plugin-vue-markdown": "^0.23.7",
    "vite-svg-loader": "^4.0.0",
    "vitest": "^0.34.1",
    "vue-tsc": "^1.8.5"
  }
}
